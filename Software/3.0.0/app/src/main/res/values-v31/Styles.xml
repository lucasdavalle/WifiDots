<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SpinnerStyle" parent="Widget.AppCompat.Spinner">
        <item name="android:fontFamily">@font/open_sans_semibold</item>
        <item name="android:textColor">#B5B5B5</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="android:windowSplashScreenAnimatedIcon">@color/colorAccent</item>
        <item name="android:windowSplashScreenBackground">@color/colorAccent</item>

        <item name="colorPrimary">#012B80</item>
        <item name="colorPrimaryDark">#000C24</item>
        <item name="colorAccent">#4F8AFF</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>