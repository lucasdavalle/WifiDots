<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SpinnerStyle" parent="Widget.AppCompat.Spinner">
        <item name="android:fontFamily">@font/open_sans_semibold</item>
        <item name="android:textColor">#B5B5B5</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="android:windowSplashScreenAnimatedIcon">@color/colorPrimary</item>
        <item name="android:windowSplashScreenBackground">@color/colorPrimary</item>

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">#FAB61E</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
</resources>